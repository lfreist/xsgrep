{
  "timer": "GNU time",
  "name": "Compressed input (LZ4 HC)",
  "description": "",
  "commands": {
    "lz4cat -> just_read": "lz4cat en.lz4 | just_read",
    "just_read": [
      "just_read",
      "en.lz4"
    ],
    "lz4cat -> GNU grep": "lz4cat en.lz4 | grep Sherlock",
    "lz4cat -> ripgrep": "lz4cat en.lz4 | rg Sherlock",
    "lz4cat -> xs grep": "lz4cat en.lz4 | xs Sherlock",
    "xspp -> xs grep": [
      "xs",
      "Sherlock",
      "en.xslz4",
      "-m", "en.xslz4.meta"
    ],
    "xspp -> xs grep --max-readers 2": [
      "xs",
      "Sherlock",
      "en.xslz4",
      "-m", "en.xslz4.meta",
      "--max-readers", "2"
    ],
    "lz4cat -> xs grep --no-mmap": "lz4cat en.lz4 | xs Sherlock --no-mmap",
    "xspp -> xs grep --no-mmap": [
      "xs",
      "Sherlock",
      "en.xslz4",
      "-m", "en.xslz4.meta",
      "--no-mmap"
    ],
    "xspp -> xs grep --max-readers 2 --no-mmap": [
      "xs",
      "Sherlock",
      "en.xslz4",
      "-m", "en.xslz4.meta",
      "--max-readers", "2",
      "--no-mmap"
    ]
  },
  "setup_cmd": [
    [
      "lz4",
      "-9",
      "en.txt",
      "en.lz4",
      "-f"
    ],
    [
      "xspp",
      "en.txt",
      "-m", "en.xslz4.meta",
      "-o", "en.xslz4",
      "-a", "lz4",
      "--hc"
    ]
  ],
  "cleanup_cmd": [
    [
      "rm",
      "en.lz4",
      "en.xslz4",
      "en.xslz4.meta"
    ]
  ]
}