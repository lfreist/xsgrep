{
  "timer": "GNU time",
  "name": "Compressed input (ZStandard)",
  "description": "",
  "commands": {
    "zstdcat -> jhust_read": "zstdcat en.zst | just_read",
    "just_read": [
      "just_read",
      "en.zst"
    ],
    "zstdcat -> GNU grep": "zstdcat en.zst | grep Sherlock",
    "zstdcat -> ripgrep": "zstdcat en.zst | rg Sherlock",
    "zstdcat -> xs grep": "zstdcat en.zst | xs Sherlock",
    "xspp -> xs grep": [
      "xs",
      "Sherlock",
      "en.xszst",
      "-m", "en.xszst.meta"
    ],
    "xspp -> xs grep --max-readers 2": [
      "xs",
      "Sherlock",
      "en.xszst",
      "-m", "en.xszst.meta",
      "--max-readers", "2"
    ]
  },
  "setup_cmd": [
    [
      "zstd",
      "en.txt",
      "-o", "en.zst",
      "-f"
    ],
    [
      "xspp",
      "en.txt",
      "-m", "en.xszst.meta",
      "-o", "en.xszst",
      "-a", "zstd"
    ]
  ],
  "cleanup_cmd": [
    [
      "rm",
      "en.zst",
      "en.xszst",
      "en.xszst.meta"
    ]
  ]
}